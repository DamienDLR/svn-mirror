\TEST{Infinite~glue~test}{
  \skip_infinite_glue:nTF {0pt plus 10mm}{\TRUE}{\FALSE}
  \skip_infinite_glue:nTF {0pt plus 1fil}{\TRUE}{\FALSE}
  \skip_infinite_glue:nTF {0pt minus 1fil}{\TRUE}{\FALSE}
  \skip_set:Nn \g_testa_skip {0pt plus 10mm minus 5mm}
  \skip_infinite_glue:nTF {\g_testa_skip}{\TRUE}{\FALSE}
  \skip_set:Nn \g_testa_skip {0pt plus 1fil}
  \skip_infinite_glue:nTF {\g_testa_skip}{\TRUE}{\FALSE}
  \skip_set:Nn \g_testa_skip {0pt plus 1 fil minus 2fill}
  \skip_infinite_glue:nTF {\g_testa_skip}{\TRUE}{\FALSE}
}

\END


