\documentclass{minimal}
\RequirePackage{expl3}
\input{regression-test}
\begin{document}
\ExplSyntaxOn

\int_new:N \g_TEST_int
\def:Npn \SEPARATOR {
  \io_put_term:x{
    ============================================================}}
\def_new:NNn \TEST 2 {
  \int_incr:N \g_TEST_int
  \SEPARATOR
  \io_put_term:x{
    TEST~\int_use:N \g_TEST_int :~\tlist_to_str:n{#1}}
  %\io_put_term:x{==~\tlist_to_str:n{#2}}
  \SEPARATOR 
  \begingroup
    \let:NN \TYPE \io_put_term:x
    #2 
  \endgroup
  \SEPARATOR \io_put_term:x{}
}

\START
\AUTHOR{Will Robertson}

\TEST{INTEGER}{
  \int_new:N  \myint
  \int_incr:N \myint
  \int_show:N \myint}

\TEST{NUM~(fake~integer)}{
  \num_new:N  \mynum
  \num_incr:N \mynum
  \num_show:N \mynum}

\TEST{DIMENSION}{
  \dim_new:N \mydim
  \dim_set:Nn \mydim {1pt}
  \dim_show:N \mydim}

\TEST{SKIP}{
  \skip_new:N \myskip
  \skip_set:Nn \myskip {1pt plus 2pt minus 3pt}
  \skip_show:N \myskip}

\TEST{TOKS}{
  \toks_new:N \mytoks
  \toks_set:Nn \mytoks { # hello $ ^ }
  \toks_show:N \mytoks}

\TEST{TLP}{
  \tlp_new:N \mytlp
  \tlp_set:Nn \mytlp {tokens}
  \tlp_show:N  \mytlp}

\TEST{SEQ}{
  \seq_new:N \myseq
  \seq_push:Nn \myseq {item1}
  \seq_push:Nn \myseq {item2}
  \seq_show:N  \myseq
  \seq_display:c {myseq}}

\TEST{CLIST}{
  \clist_new:N \myclist
  \clist_push:Nn \myclist {item1}
  \clist_push:Nn \myclist {item2}
  \clist_show:N  \myclist
  \clist_display:c {myclist}}

\TEST{PROP}{
  \prop_new:N \myprop
  \prop_put:Nnn \myprop {key1} {val1}
  \prop_put:Nnn \myprop {key2} {val2}
  \prop_show:N  \myprop
  \prop_display:c {myprop}}


\END
