\AUTHOR{Will~Robertson}


%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% This test is separate because we need 
% to load the full expl3 bundle, and I 
% prefer not to do that to check 
% individual modules.
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

\OMIT
\def:Npn    \regmacro {}
\dim_new:N  \regdim
\skip_new:N \regskip
\int_new:N  \regint
\toks_new:N \regtoks
