\TESTEXP{to_str}{
  \tlist_to_str:n { abc } \NEWLINE
  \tlist_to_str:n { \ERROR } \NEWLINE
  \tlist_to_str:n { # }
}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

\TEST{if_eq}{
  \tlist_if_eq:nnTF {}{}               {\TRUE}{\FALSE}
  \tlist_if_eq:nnTF {~}{~~}            {\TRUE}{\FALSE}
  \tlist_if_eq:nnTF {abc}{abc}         {\TRUE}{\FALSE}
  \tlist_if_eq:nnTF {abc}{abc~}        {\TRUE}{\FALSE}
  \tlist_if_eq:nnTF {abc#}{abc#}       {\TRUE}{\FALSE}
  \tlist_if_eq:nnTF {abc\par}{abc\par} {\TRUE}{\FALSE}
  \SEPARATOR
  \tlist_if_eq:nnTF {}{}               {\TRUE}
  \tlist_if_eq:nnTF {~}{~~}            {\TRUE}
  \tlist_if_eq:nnT  {abc}{abc}         {\TRUE}
  \tlist_if_eq:nnT  {abc#}{abc#}       {\TRUE}
  \tlist_if_eq:nnT  {abc\par}{abc\par} {\TRUE}
  \SEPARATOR
  \tlist_if_eq:nnF  {abc}{abc~}               {\FALSE}
  \SEPARATOR
  \tlist_if_eq:nnTF {}{}               {\FALSE}{\TRUE}
  \tlist_if_eq:nnTF {~}{~~}            {\FALSE}{\TRUE}
  \tlist_if_eq:nnTF {abc}{abc}         {\FALSE}{\TRUE}
  \tlist_if_eq:nnTF {abc}{abc~}        {\FALSE}{\TRUE}
  \tlist_if_eq:nnTF {abc#}{abc#}       {\FALSE}{\TRUE}
  \tlist_if_eq:nnTF {abc\par}{abc\par} {\FALSE}{\TRUE}
}

\OMIT
\tl_set:Nn \l_tmpa_tl {a}
\tl_set:Nn \l_tmpb_tl {\l_tmpa_tl}
\tl_set:Nn \l_tmpc_tl {\l_tmpb_tl}
\tl_set:Nn \l_tmpd_tl {\l_tmpc_tl}
\tl_set:Nn \l_tmpe_tl {a}
\TIMO

\TEST{if_eq~variants}{
  \tlist_if_eq:noTF {ab}{\l_tmpa_tl b} {\TRUE}{\FALSE}
  \tlist_if_eq:noT  {ab}{\l_tmpa_tl b} {\TRUE}
  \tlist_if_eq:noF  {ab}{\l_tmpa_tl Z} {\FALSE}
  \SEPARATOR
  \tlist_if_eq:onTF {\l_tmpa_tl b}{ab} {\TRUE}{\FALSE}
  \tlist_if_eq:onT  {\l_tmpa_tl b}{ab} {\TRUE}
  \tlist_if_eq:onF  {\l_tmpa_tl Z}{ab} {\FALSE}
  \SEPARATOR
  \tlist_if_eq:oxTF {\l_tmpa_tl}{\l_tmpd_tl}   {\TRUE}{\FALSE}
  \tlist_if_eq:oxT  {\l_tmpa_tl}{\l_tmpd_tl}   {\TRUE}
  \tlist_if_eq:oxF  {\l_tmpa_tl}{\l_tmpd_tl Z} {\FALSE}
  \SEPARATOR
  \tlist_if_eq:xoTF {\l_tmpd_tl}  {\l_tmpa_tl} {\TRUE}{\FALSE}
  \tlist_if_eq:xoT  {\l_tmpd_tl}  {\l_tmpa_tl} {\TRUE}
  \tlist_if_eq:xoF  {\l_tmpd_tl Z}{\l_tmpa_tl} {\FALSE}
  \SEPARATOR
  \tlist_if_eq:xxTF {\l_tmpd_tl}{\l_tmpc_tl}   {\TRUE}{\FALSE}
  \tlist_if_eq:xxT  {\l_tmpd_tl}{\l_tmpc_tl}   {\TRUE}
  \tlist_if_eq:xxF  {\l_tmpd_tl}{\l_tmpc_tl Z} {\FALSE}
  \SEPARATOR
  \tlist_if_eq:nxTF {ab}{\l_tmpc_tl b} {\TRUE}{\FALSE}
  \tlist_if_eq:xnT  {\l_tmpd_tl b}{ab} {\TRUE}
  \tlist_if_eq:nxF  {ab}{\l_tmpc_tl Z} {\FALSE}
  \SEPARATOR
  \tlist_if_eq:ooTF {\l_tmpa_tl b}{\l_tmpe_tl b} {\TRUE}{\FALSE}
  \tlist_if_eq:ooT  {\l_tmpa_tl b}{\l_tmpe_tl b} {\TRUE}
  \tlist_if_eq:ooF  {\l_tmpa_tl b}{\l_tmpe_tl Z} {\FALSE}
}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

\tl_set:Nn \l_tmpa_tl {}
\tl_set:Nn \l_tmpb_tl {~}
\tl_set:Nn \l_tmpc_tl {X}
\TESTEXP{if_empty~(expect~T/F/F~repeated)}{
  \tlist_if_empty_p:n { }~
  \tlist_if_empty_p:n {~}~
  \tlist_if_empty_p:n {X}\NEWLINE
  \tlist_if_empty:nTF { } {\TRUE}{\FALSE} \NEWLINE
  \tlist_if_empty:nTF {~} {\TRUE}{\FALSE} \NEWLINE
  \tlist_if_empty:nTF {X} {\TRUE}{\FALSE} \NEWLINE
  \NEWLINE
  \tlist_if_empty_p:o { \l_tmpa_tl }~
  \tlist_if_empty_p:o { \l_tmpb_tl }~
  \tlist_if_empty_p:o { \l_tmpc_tl }\NEWLINE
  \tlist_if_empty:oTF { \l_tmpa_tl } {\TRUE}{\FALSE} \NEWLINE
  \tlist_if_empty:oTF { \l_tmpb_tl } {\TRUE}{\FALSE} \NEWLINE
  \tlist_if_empty:oTF { \l_tmpc_tl } {\TRUE}{\FALSE} \NEWLINE
}

\TESTEXP{if_blank~(expect~T/T/F~repeated)}{
  \tlist_if_blank_p:n { }~
  \tlist_if_blank_p:n {~}~
  \tlist_if_blank_p:n {X}\NEWLINE
  \tlist_if_blank:nTF { } {\TRUE}{\FALSE} \NEWLINE
  \tlist_if_blank:nTF {~} {\TRUE}{\FALSE} \NEWLINE
  \tlist_if_blank:nTF {X} {\TRUE}{\FALSE} \NEWLINE
  \NEWLINE
  \tlist_if_blank_p:o { \l_tmpa_tl }~
  \tlist_if_blank_p:o { \l_tmpb_tl }~
  \tlist_if_blank_p:o { \l_tmpc_tl }\NEWLINE
  \tlist_if_blank:oTF { \l_tmpa_tl } {\TRUE}{\FALSE} \NEWLINE
  \tlist_if_blank:oTF { \l_tmpb_tl } {\TRUE}{\FALSE} \NEWLINE
  \tlist_if_blank:oTF { \l_tmpc_tl } {\TRUE}{\FALSE} \NEWLINE
}

