\TEST{if_in}{
  \tlist_if_in:nnTF {123abc\par}  {abc\par} {\TRUE}{\FALSE}
  \tlist_if_in:nnTF {123ab~cdef} {ab~c}     {\TRUE}{\FALSE}
  \SEPARATOR
  \tl_set:Nn \l_tmpa_tl { abc~{abc}~abc }
  \tlist_if_in:onTF {\l_tmpa_tl}  {abc~} {\TRUE}{\FALSE}
}

