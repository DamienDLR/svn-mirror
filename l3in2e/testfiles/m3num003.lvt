\TEST{use}{
  \TYPE {\num_use:N \l_tmpa_num}
  \TYPE {\num_use:N \l_tmpz_num}
  \TYPE {\num_use:c {l_tmpb_num}}
  \TYPE {\num_use:c {l_tmpy_num}}
}

\TEST{set_eq}{
  {
    \num_set:Nn \l_tmpa_num {7}
    \num_set_eq:NN \l_tmpb_num \l_tmpa_num
    \TYPE{ \num_use:N \l_tmpb_num }
    \num_set:Nn \l_tmpa_num {8}
    \num_set_eq:cN {l_tmpb_num} \l_tmpa_num
    \TYPE{ \num_use:c {l_tmpb_num} }
    \num_set:cn {l_tmpa_num} {9}
    \num_set_eq:Nc \l_tmpb_num {l_tmpa_num}
    \TYPE{ \num_use:N \l_tmpb_num }
    \num_set:cn {l_tmpa_num} {103}
    \num_set_eq:cc {l_tmpb_num} {l_tmpa_num}
    \TYPE{ \num_use:c {l_tmpb_num} }
  }
  \TYPE{ \num_use:c {l_tmpb_num} }
}

\TESTEXP{eval}{
  \num_value:w \num_eval:w 50 / 5 \num_eval_end: \NEWLINE
  \num_value:w \num_eval:w  5 / 3 \num_eval_end: \NEWLINE
  \num_value:w \num_eval:w  4 / 3 \num_eval_end: \NEWLINE
  \num_value:w \num_eval:w  4 + 3 \num_eval_end: \NEWLINE
  \num_value:w \num_eval:w -4 + 3 \num_eval_end: \NEWLINE
}


\TESTEXP{max/min/abs}{
  \num_max_of:nn {0} {0} \NEWLINE
  \num_max_of:nn {-999} {0} \NEWLINE
  \num_max_of:nn {-999} {-5+6} \NEWLINE
  \num_max_of:nn {999} {0} \NEWLINE
  \num_max_of:nn {999} {5} \NEWLINE
  === \NEWLINE
  \num_min_of:nn {0} {0} \NEWLINE
  \num_min_of:nn {-999} {0} \NEWLINE
  \num_min_of:nn {-999-1} {-15+50098} \NEWLINE
  \num_min_of:nn {999} {0} \NEWLINE
  === \NEWLINE
  \num_abs:n {0} \NEWLINE
  \num_abs:n {-7} = \num_value:w \num_abs:n {-7} \NEWLINE
  \num_abs:n {-15+30} = \num_value:w \num_abs:n {-15+30} \NEWLINE
  \num_abs:n {-15-30} = \num_value:w \num_abs:n {-15-30} \NEWLINE
}

