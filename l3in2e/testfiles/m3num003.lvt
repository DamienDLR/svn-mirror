\TEST{set_eq}{
  {
    \num_set:Nn \l_tmpa_num {7}
    \num_set_eq:NN \l_tmpb_num \l_tmpa_num
    \TYPE{ \num_use:N \l_tmpb_num }
    \num_set:Nn \l_tmpa_num {8}
    \num_set_eq:cN {l_tmpb_num} \l_tmpa_num
    \TYPE{ \num_use:c {l_tmpb_num} }
    \num_set:cn {l_tmpa_num} {9}
    \num_set_eq:Nc \l_tmpb_num {l_tmpa_num}
    \TYPE{ \num_use:N \l_tmpb_num }
    \num_set:cn {l_tmpa_num} {103}
    \num_set_eq:cc {l_tmpb_num} {l_tmpa_num}
    \TYPE{ \num_use:c {l_tmpb_num} }
  }
  \TYPE{ \num_use:c {l_tmpb_num} }
}

\TESTEXP{eval}{
  \intexpr_eval:n {50 / 5} \NEWLINE
  \intexpr_eval:n {5 / 3} \NEWLINE
  \intexpr_eval:n {4 / 3} \NEWLINE
  \intexpr_eval:n { 4 + 3} \NEWLINE
  \intexpr_eval:n {-4+3} \NEWLINE
}


\TESTEXP{max/min/abs}{
  \intexpr_max:nn {0} {0} \NEWLINE
  \intexpr_max:nn {-999} {0} \NEWLINE
  \intexpr_max:nn {-999} {-5+6} \NEWLINE
  \intexpr_max:nn {999} {0} \NEWLINE
  \intexpr_max:nn {999} {5} \NEWLINE
  === \NEWLINE
  \intexpr_min:nn {0} {0} \NEWLINE
  \intexpr_min:nn {-999} {0} \NEWLINE
  \intexpr_min:nn {-999-1} {-15+50098} \NEWLINE
  \intexpr_min:nn {999} {0} \NEWLINE
  === \NEWLINE
  \intexpr_abs:n {0} \NEWLINE
  \intexpr_abs:n {-7} = 7 \NEWLINE
  \intexpr_abs:n {-15+30} = 15 \NEWLINE
  \intexpr_abs:n {-15-30} = 45 \NEWLINE
}

