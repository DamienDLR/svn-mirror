\TESTEXP{tl_if_single}{
  \tl_if_single:nTF {}               ?F
  \tl_if_single:nTF {~}              T?
  \exp_args:Nf \tl_if_single:nTF
    { \prg_replicate:nn {5} {~} }    T?
  \tl_if_single:nTF {\foo}           T?
  \tl_if_single:nTF {^^65}           T?
  \tl_if_single:nTF {~\foo}          T?
  \tl_if_single:nTF { * ~ }          ?F
  \tl_if_single:nTF { ~*~ }          ?F
  \tl_if_single:nTF { A B }          ?F
  \tl_if_single:nTF { { } }          T?
  \tl_if_single:nTF { { }~}          ?F
  \tl_if_single:nTF {~{ } }          T?
  \tl_if_single:nTF {~{ }~}          ?F
  \tl_if_single:nTF { {A} }          T?
  \tl_if_single:nTF { {A}~}          ?F
  \tl_if_single:nTF {~{A} }          T?
  \tl_if_single:nTF {~{A}~}          ?F
  \tl_if_single:nTF {A{B} }          ?F
  \tl_if_single:nTF { {B}A}          ?F
  \tl_if_single:nTF {{} {}}          ?F
}

